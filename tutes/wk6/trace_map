


map([1,3,5,7,9], square);           // [1,3,5,7,9]
   square(1);                       // [1,3,5,7,9]
   map([3,5,7,9], square);          // [1,3,5,7,9]
       square(3);                   // [1,9,5,7,9]
       map([5,7,9], square);        // [1,9,5,7,9]
           square(5);               // [1,9,25,7,9]
           map([7,9], square);      // [1,9,25,7,9]
               square(7);           // [1,9,25,49,9]
               map([9], square)     // [1,9,25,49,9]
                   square(9);       // [1,9,25,49,81]
                   map([], square); // [1,9,25,49,81]
